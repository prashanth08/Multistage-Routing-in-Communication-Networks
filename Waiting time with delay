unsigned int randr(unsigned int min, unsigned int max)
{
       double scaled = (double)rand()/RAND_MAX;

       return (max - min +1)*scaled + min;
}
double exponential(double x)
{

double z;
do
{
    float z=randr(0,10) * 0.1;
}
while((z<0) && (z>1));
return (-x*log(z));
}
